<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sonus.CadastroArtistas"
             Title="CadastroArtistas">
    <VerticalStackLayout>

        <Label Text="Digite o nome do artista: "/>
        <Entry x:Name="EntryNome" />

        <Label Text="Insira a url da imagem: "/>
        <Entry x:Name="EntryUrl"/>

        <Button Text="Salvar" Clicked="Salvar_Clicked"/>
        


    </VerticalStackLayout>
</ContentPage>